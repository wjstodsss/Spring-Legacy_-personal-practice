<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <typeAliases>
        <typeAlias type="com.blacksmith.ecommerce.admin.login.domain.MemberDTO" alias="member"></typeAlias>
        <typeAlias type="com.blacksmith.ecommerce.admin.user.domain.UserDTO" alias="user"></typeAlias>
        <!-- 별칭 추가 -->
        <typeAlias type="com.blacksmith.ecommerce.admin.product.domain.ProductDTO" alias="product"></typeAlias>
    </typeAliases>

    <mappers>
        <mapper resource="mapper/loginMapper.xml"/>
        <mapper resource="mapper/userMapper.xml"/>
        <!-- 매퍼 추가 -->
        <mapper resource="mapper/productMapper.xml"/>
    </mappers>
    
    
</configuration>
